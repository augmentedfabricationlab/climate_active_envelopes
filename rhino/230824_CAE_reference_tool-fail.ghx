import math as m
from compas.geometry import Frame, Point, Rotation, Transformation, Translation
from compas.utilities import normalize_values
#from climate_aware_robotic_envelope.assembly import Assembly

#from climate_active_envelopes.assembly import BRICK_Assembly as Assembly


length = assembly.get_attribute('length')
height = assembly.get_attribute('height')
width = assembly.get_attribute('width')
mortar = assembly.get_attribute('mortar')

"""
c_pts = []
for j, face in enumerate(mesh_wall.faces()):
    vertex_centroid = mesh_wall.face_centroid(face)
    points = rg.Point3d(vertex_centroid[0],vertex_centroid[1], vertex_centroid[2])
    c_pts.append(points)

for i, points in enumerate(c_pts):
    transf_values = normalize_values(transf_values, -0.10, 0.0)
    t_val = transf_values[i]
    t_pl = rg.Plane(rg.Point3d(points[0],t_val,points[2]), rg.Vector3d(1, 0, 0), rg.Vector3d(0, 1, 0))
    t_frame = Frame(t_pl.Origin, t_pl.XAxis, t_pl.YAxis)
"""
if generate:
    for i, row in enumerate(plane_grid): # number of layers and all planes
        for j, plane in enumerate(row): #j = planes / layer
            frame = Frame(plane.Origin, plane.XAxis, plane.YAxis)
            frame_pt = frame.point

            if transform == True:
                for t_pt in t_pts:
                    dist =  t_pt.DistanceTo(rg.Point3d(frame.point[0], frame.point[1], frame.point[2]))

                if i%2 == 0 and j%3 == 1 and j%2 == 1:
                    plane.Translate(plane.XAxis*-m.radians(dist*2))

                if i%2 == 1  and j%3 != 0 and j%3 != 1 and j%2 == 1:
                    plane.Translate(plane.XAxis*-m.radians(dist*2))
                T = Transformation.from_frame_to_frame(Frame.worldXY(), Frame(plane.Origin, plane.XAxis, plane.YAxis))
            else: 
                T = Transformation.from_frame_to_frame(Frame.worldXY(), frame)


            if i%2 == 1 and j%2 == 1 and j%3 != 1:
                c = elem_full.transformed(T)
                assembly.add_element(c)
            elif i%2 == 0 and j%2 == 1 and j%3 != 0:
                c = elem_full.transformed(T)
                assembly.add_element(c)
            else:
                c = elem_ins.transformed(T)
                assembly.add_element(c)

