from compas.geometry import Point
from compas.geometry import Translation, Frame, Point, Rotation, Transformation
from compas_ghpython import draw_frame
from compas.artists import Artist
from compas.datastructures import Network
from climate_active_envelopes.assembly import ReferenceModel, Assembly
from climate_active_envelopes.assembly import ReferenceElement, Brick
import math as m

assembly = Assembly()

