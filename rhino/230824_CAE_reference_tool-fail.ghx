for course_frames in frames_grid:
    course_planes = []
    for frame in course_frames:
        origin = rg.Point3d(frame.point.x, frame.point.y, frame.point.z)
        xaxis = rg.Vector3d(frame.xaxis.x, frame.xaxis.y, frame.xaxis.z)
        yaxis = rg.Vector3d(frame.yaxis.x, frame.yaxis.y, frame.yaxis.z)
        plane = rg.Plane(origin, xaxis, yaxis)
        course_planes.append(plane)
    planes_grid.append(course_planes)

